{
  "name": "angular-openvidu",
  "description": "Simple, robust, OpenVidu videochat room component for Angular",
  "version": "0.5.0",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openvidu/angular-openvidu.git"
  },
  "author": "Alex <alxmorais8@msn.com>",
  "bugs": {
    "url": "https://github.com/openvidu/angular-openvidu/issues"
  },
  "main": "index.bundle.js",
  "module": "index.js",
  "jsnext:main": "index.js",
  "types": "index.d.ts",
  "typings": "index.d.ts",
  "scripts": {
	"clean": "rimraf dist/",
    "lint": "tslint -c tslint.json src/**/*.ts",
	"css": "gulp css",
    "build": "npm run clean && npm run css && tsc -p tsconfig.json && ts-node tools/inline-files.ts && tsc -p tsconfig.json && rollup -c rollup.config.js dist/index.js > dist/index.bundle.js && ngc -p tsconfig.json && mv src/openvidu.component.ts.bak src/openvidu.component.ts && mv src/stream.component.ts.bak src/stream.component.ts && cp package.json dist && cp README.md dist",
    "test": "mocha --require ts-node/register src/**/*.spec.ts --recursive"
  },
  "dependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/material": "^2.0.0-beta.2",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "hammerjs": "^2.0.8",
    "openvidu-browser": "^0.0.2",
    "rxjs": "^5.0.1",
	"angular-bigscreen": "^0.1.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "^1.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@types/hammerjs": "^2.0.34",
    "@types/jasmine": "2.5.41",
    "@types/node": "^7.0.8",
    "@types/requirejs": "^2.1.28",
    "@types/screenfull": "^3.0.28",
	"chai": "^3.5.0",
    "codelyzer": "~2.0.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-less": "^3.3.0",
    "gulp-uglify": "^2.1.0",
    "gulp-util": "^3.0.8",
    "hammerjs": "^2.0.8",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "3.2.0",
    "karma": "1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.6.0",
	"mocha": "^3.2.0",
    "protractor": "~5.1.1",
    "rimraf": "^2.6.1",
    "rollup": "^0.41.5",
    "ts-node": "2.1.0",
    "tslint": "^4.4.2",
    "typescript": "~2.2.1"
  },
  "keywords": [
    "openvidu",
    "angular",
    "browser",
    "live",
    "video",
    "chat"
  ]
}
