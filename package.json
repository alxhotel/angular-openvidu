{
  "name": "angular-openvidu",
  "description": "Simple, robust, openvidu videochat room component for Angular",
  "version": "0.5.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openvidu/angular-openvidu.git"
  },
  "author": "Alex <alxmorais8@msn.com>",
  "bugs": {
    "url": "https://github.com/openvidu/angular-openvidu/issues"
  },
  "main": "dist/index.bundle.js",
  "module": "dist/index.js",
  "jsnext:main": "dist/index.js",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "scripts": {
    "lint": "tslint 'src/**/*.ts'",
    "clean": "rimraf dist",
    "build": "npm-run-all --serial clean build:ngc build:umd build:app",
    "build:ngc": "ngc -p tsconfig.ngc.json",
    "build:umd": "NODE_ENV=prod webpack",
    "build:app": "NODE_ENV=prod webpack --config app/webpack.config",
	"prepublish": "gulp css && typings install && npm test"
  },
  "dependencies": {
    "@angular/common": "^4.0.0-rc.2",
    "@angular/compiler": "^4.0.0-rc.2",
    "@angular/core": "^4.0.0-rc.2",
    "@angular/forms": "^4.0.0-rc.2",
    "@angular/http": "^4.0.0-rc.2",
    "@angular/material": "^2.0.0-beta.2",
    "@angular/platform-browser": "^4.0.0-rc.2",
    "@angular/platform-browser-dynamic": "^4.0.0-rc.2",
    "@angular/router": "^4.0.0-rc.2",
    "hammerjs": "^2.0.8",
    "openvidu-browser": "^0.0.2",
    "screenfull": "^3.0.2",
    "wolfy87-eventemitter": "^4.2.9"
  },
  "devDependencies": {
    "@angular/cli": "^1.0.0-rc.1",
    "@angular/compiler-cli": "^4.0.0-rc.2",
    "@types/jasmine": "2.5.43",
    "@types/node": "^7.0.5",
    "codelyzer": "~2.0.1",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "3.2.0",
    "karma": "1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.6.0",
    "protractor": "~5.1.1",
    "ts-node": "2.1.0",
    "tslint": "^4.4.2",
    "typescript": "~2.2.1",
    "@types/hammerjs": "^2.0.34",
    "@types/requirejs": "^2.1.28",
    "@types/screenfull": "^3.0.28",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-less": "^3.3.0",
    "gulp-uglify": "^2.1.0",
    "gulp-util": "^3.0.8",
    "hammerjs": "^2.0.8"
